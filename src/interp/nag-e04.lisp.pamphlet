(DEFUN |e04ucfSolve,fb| (|i| |j|)
  (PROG (|anam|)
    (RETURN
      (SEQ (SPADLET |anam|
                    (INTERN (STRCONC (MAKESTRING "a") (STRINGIMAGE |i|)
                                     (STRINGIMAGE |j|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 8
                                    (CONS 0
                                     (CONS |anam| (CONS 'F NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |e04ucfSolve,fa| (|i| |n|)
  (PROG (|prefix| |labelList|)
    (RETURN
      (SEQ (SPADLET |labelList|
                    (PROG (G167345)
                      (SPADLET G167345 NIL)
                      (RETURN
                        (DO ((|j| 1 (QSADD1 |j|)))
                            ((QSGREATERP |j| |n|) G167345)
                          (SEQ (EXIT (SETQ G167345
                                      (APPEND G167345
                                       (|e04ucfSolve,fb| |i| |j|)))))))))
           (SPADLET |prefix| (MAKESTRING "\\newline "))
           (EXIT (SPADLET |labelList|
                          (CONS (CONS '|text| |prefix|) |labelList|)))))))

