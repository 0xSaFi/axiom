(DEFUN |e04dgfGen| (|htPage|)
  (PROG (|n| |optional| |ifail| |alist| |ver| |sto| |sta| |pr| |op|
             |ma| |nolist| |lis| |dummy| |lin| |it| |fu| |es| |temp|
             |xlist| |y| |xstring| |f| |prefix| |middle|)
    (RETURN
      (SEQ (PROGN
             (SPADLET |n| (|htpProperty| |htPage| '|n|))
             (SPADLET |optional| (|htpProperty| |htPage| '|optional|))
             (SPADLET |ifail| (|htpProperty| |htPage| '|ifail|))
             (SPADLET |alist| (|htpInputAreaAlist| |htPage|))
             (SPADLET |y| |alist|)
             (COND
               ((BOOT-EQUAL |optional| '0)
                (SPADLET |es| (MAKESTRING "1.0"))
                (SPADLET |ma| (MAKESTRING "1.0E+20"))
                (SPADLET |op| (MAKESTRING "3.26E-12"))
