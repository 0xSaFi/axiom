(DEFUN |e04fdfGen| (|htPage|)
  (PROG (|n| |m| |liw| |lw| |ifail| |alist| |xlist| |xstring| |temp|
             |ulist| |y| |ustring| |prefix| |middle|)
    (RETURN
      (SEQ (PROGN
             (SPADLET |n| (|htpProperty| |htPage| '|n|))
             (SPADLET |m| (|htpProperty| |htPage| '|m|))
