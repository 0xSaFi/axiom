(DEFUN |d02kefSolve,fa| (|i|)
  (PROG (|xpnam|)
    (RETURN
      (SEQ (setq |xpnam|
                    (INTERN (STRCONC "xp"
                                     (princ-to-string |i|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 10
                                    (CONS '|0.0|
                                     (CONS |xpnam| (CONS 'EM NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |d02kefSolve,fb| (|i|)
  (PROG (|ylnam|)
    (RETURN
      (SEQ (setq |ylnam|
                    (INTERN (STRCONC "yl"
