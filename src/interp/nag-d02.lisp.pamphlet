(DEFUN |d02gafSolve,fa| (|i|)
  (PROG (|prefix| |funct| |nam|)
    (RETURN
      (SEQ (setq |prefix| "\\newline {\\em Function ")
           (setq |prefix|
                    (STRCONC |prefix| (princ-to-string |i|)
                             ":} \\space{1}"))
           (setq |funct|
                    (STRCONC "Y[" (princ-to-string |i|) ']))
           (setq |nam|
                    (INTERN (STRCONC "n"
                                     (princ-to-string |i|))))
           (EXIT (CONS (CONS '|text| |prefix|)
                       (CONS (CONS '|bcStrings|
                                   (CONS
                                    (CONS 42
                                     (CONS |funct|
                                      (CONS |nam| (CONS 'EM NIL))))
                                    NIL))
                             NIL)))))))

(DEFUN |d02gafSolve,fc| (|i| |j|)
  (PROG (|unam|)
    (RETURN
      (SEQ (setq |unam|
                    (INTERN (STRCONC "u" (princ-to-string |i|)
                                     (princ-to-string |j|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 6
                                    (CONS 0
                                     (CONS |unam| (CONS 'F NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |d02gafSolve,fb| (|i|)
  (PROG (|prefix| |labelList|)
    (RETURN
      (SEQ (setq |labelList|
                    (PROG (G166576)
                      (setq G166576 NIL)
                      (RETURN
                        (DO ((|j| 1 (QSADD1 |j|)))
                            ((QSGREATERP |j| 2) G166576)
                          (SEQ (EXIT (SETQ G166576
                                      (APPEND G166576
                                       (|d02gafSolve,fc| |i| |j|)))))))))
           (setq |prefix| "\\newline ")
           (EXIT (setq |labelList|
                          (CONS (CONS '|text| |prefix|) |labelList|)))))))

(DEFUN |d02gafSolve,fe| (|i| |j|)
  (PROG (|vnam|)
    (RETURN
      (SEQ (setq |vnam|
                    (INTERN (STRCONC "v" (princ-to-string |i|)
                                     (princ-to-string |j|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 6
                                    (CONS 0
                                     (CONS |vnam| (CONS 'F NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |d02gafSolve,fd| (|i|)
  (PROG (|prefix| |labelList|)
    (RETURN
      (SEQ (setq |labelList|
                    (PROG (G166599)
                      (setq G166599 NIL)
                      (RETURN
                        (DO ((|j| 1 (QSADD1 |j|)))
                            ((QSGREATERP |j| 2) G166599)
                          (SEQ (EXIT (SETQ G166599
                                      (APPEND G166599
                                       (|d02gafSolve,fe| |i| |j|)))))))))
           (setq |prefix| "\\newline ")
           (EXIT (setq |labelList|
                          (CONS (CONS '|text| |prefix|) |labelList|)))))))

(DEFUN |d02gafSolve,ff| (|i|)
  (PROG (|xnam|)
    (RETURN
      (SEQ (setq |xnam|
                    (INTERN (STRCONC "x"
                                     (princ-to-string |i|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 8
                                    (CONS '|0.0|
                                     (CONS |xnam| (CONS 'F NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |d02gafSolve| (|htPage|)
  (PROG (|n| |a| |b| |mnp| |np| |lw| |liw| |tol| |aerror| |afail|
             |berror| |bfail| |cerror| |cfail| |ifail| |funcList|
             |middle| |uList| |mid| |vList| |end| |xList|
             |equationPart| |page|)
  (declare (special |$EmptyMode| |$bcParseOnly|))
    (RETURN
      (SEQ (PROGN
             (setq |n|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|n|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|n|)))))
             (setq |a| (|htpLabelInputString| |htPage| '|a|))
             (setq |b| (|htpLabelInputString| |htPage| '|b|))
             (setq |mnp|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|mnp|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|mnp|)))))
             (setq |np|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|np|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|np|)))))
             (setq |lw|
                      (+ (+ (TIMES |mnp|
                                         (+
                                          (+
                                           (TIMES (TIMES 3 |n|) |n|)
                                           (TIMES 6 |n|))
                                          2))
                                  (TIMES (TIMES 4 |n|) |n|))
                            (TIMES 4 |n|)))
             (setq |liw|
                      (+ (+ (+ (TIMES |mnp|
                                         (+ (TIMES 2 |n|) 1))
                                        (TIMES |n| |n|))
                                  (TIMES 4 |n|))
                            2))
             (setq |tol| (|htpLabelInputString| |htPage| '|tol|))
             (setq |aerror| (|htpButtonValue| |htPage| '|afail|))
             (setq |afail|
                      (COND
                        ((BOOT-EQUAL |aerror| '|azero|) '0)
                        ('T '1)))
             (setq |berror| (|htpButtonValue| |htPage| '|bfail|))
             (setq |bfail|
                      (COND
                        ((BOOT-EQUAL |berror| '|bone|) '1)
                        ('T '0)))
             (setq |cerror| (|htpButtonValue| |htPage| '|cfail|))
             (setq |cfail|
                      (COND
                        ((BOOT-EQUAL |cerror| '|cone|) '1)
                        ('T '0)))
             (setq |ifail|
                      (+ (+ (TIMES 100 |cfail|)
                                  (TIMES 10 |bfail|))
                            |afail|))
             (COND
               ((BOOT-EQUAL |n| '3)
                (|d02gafDefaultSolve| |htPage| |a| |b| |mnp| |np| |lw|
                    |liw| |tol| |ifail|))
               ('T
                (setq |funcList|
                         (PROG (G166622)
                           (setq G166622 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166622)
                               (SEQ (EXIT
                                     (SETQ G166622
                                      (APPEND G166622
                                       (|d02gafSolve,fa| |i|)))))))))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter known or estimated ")
                (setq |middle|
                         (STRCONC |middle|
                                                                        "values of \\htbitmap{yi} at a and b, "))
                (setq |middle|
                         (STRCONC |middle|
                                  '|{\\it U(n,2)}. [\\htbitmap{yi}(a) in the first |))
                (setq |middle|
                         (STRCONC |middle|
                                  '|column, \\htbitmap{yi}(b) in the second.] |))
                (setq |middle| (STRCONC |middle| '|\\newline |))
                (setq |uList|
                         (PROG (G166630)
                           (setq G166630 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166630)
                               (SEQ (EXIT
                                     (SETQ G166630
                                      (APPEND G166630
                                       (|d02gafSolve,fb| |i|)))))))))
                (setq |uList|
                         (CONS (CONS '|text| |middle|) |uList|))
                (setq |mid|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter {\\it V(n,2)}. ")
                (setq |mid|
                         (STRCONC |mid|
                                                                        "If U(i,j) is known V(i,j) "))
                (setq |mid|
                         (STRCONC |mid|
                                                                        "= 0.0, else V(i,j) = 1.0. \\newline "))
                (setq |vList|
                         (PROG (G166638)
                           (setq G166638 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166638)
                               (SEQ (EXIT
                                     (SETQ G166638
                                      (APPEND G166638
                                       (|d02gafSolve,fd| |i|)))))))))
                (setq |vList| (CONS (CONS '|text| |mid|) |vList|))
                (setq |xList|
                         (PROG (G166646)
                           (setq G166646 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |mnp|) G166646)
                               (SEQ (EXIT
                                     (SETQ G166646
                                      (APPEND G166646
                                       (|d02gafSolve,ff| |i|)))))))))
                (setq |end|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter the initial mesh ")
                (setq |end|
                         (STRCONC |end|
                                                                        "{\\it X(mnp)}: \\newline "))
                (setq |xList| (CONS (CONS '|text| |end|) |xList|))
                (setq |equationPart|
                         (CONS '(|domainConditions|
                                    (|isDomain| EM |$EmptyMode|)
                                    (|isDomain| S (|String|))
                                    (|isDomain| F (|Float|))
                                    (|isDomain| I (|Integer|)))
                               (APPEND |funcList|
                                       (APPEND |uList|
                                        (APPEND |vList| |xList|)))))
                (setq |page|
                         (|htInitPage|
                                                              "D02GAF - ODEs, boundary value problem, finite difference technique with deferred correction, simple nonlinear problem"
                             NIL))
