                (setq |xpList|
                         (PROG (G167297)
                           (setq G167297 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |m|) G167297)
                               (SEQ (EXIT
                                     (SETQ G167297
                                      (APPEND G167297
                                       (|d02kefSolve,fa| |i|)))))))))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Value of {\\it p} for COEFFN:")
