(DEFUN |d02gbfSolve| (|htPage|)
  (PROG (|n| |a| |b| |mnp| |np| |lw| |liw| |tol| |aerror| |afail|
             |berror| |bfail| |cerror| |cfail| |ifail| |cList| |dList|
             |gamList| |middle| |fList| |mid| |prefix| |gList| |end|
             |xList| |equationPart| |page|)
  (declare (special |$EmptyMode| |$bcParseOnly|))
    (RETURN
      (SEQ (PROGN
             (setq |n|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|n|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|n|)))))
             (setq |a| (|htpLabelInputString| |htPage| '|a|))
             (setq |b| (|htpLabelInputString| |htPage| '|b|))
             (setq |mnp|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|mnp|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|mnp|)))))
             (setq |np|
                      (COND
                        (|$bcParseOnly|
                            (PARSE-INTEGER
                                (|htpLabelInputString| |htPage| '|np|)))
                        ('T
                         (|objValUnwrap|
                             (|htpLabelSpadValue| |htPage| '|np|)))))
             (setq |lw|
                      (+ (+ (TIMES |mnp|
                                         (+
                                          (+
                                           (TIMES (TIMES 3 |n|) |n|)
                                           (TIMES 5 |n|))
                                          2))
                                  (TIMES (TIMES 3 |n|) |n|))
                            (TIMES 5 |n|)))
             (setq |liw|
                      (+ (TIMES |mnp| (+ (TIMES 2 |n|) 1)) |n|))
             (setq |tol| (|htpLabelInputString| |htPage| '|tol|))
             (setq |aerror| (|htpButtonValue| |htPage| '|afail|))
             (setq |afail|
                      (COND
                        ((BOOT-EQUAL |aerror| '|azero|) '0)
                        ('T '1)))
             (setq |berror| (|htpButtonValue| |htPage| '|bfail|))
             (setq |bfail|
                      (COND
                        ((BOOT-EQUAL |berror| '|bone|) '1)
                        ('T '0)))
             (setq |cerror| (|htpButtonValue| |htPage| '|cfail|))
             (setq |cfail|
                      (COND
                        ((BOOT-EQUAL |cerror| '|cone|) '1)
                        ('T '0)))
             (setq |ifail|
                      (+ (+ (TIMES 100 |cfail|)
                                  (TIMES 10 |bfail|))
                            |afail|))
             (COND
               ((BOOT-EQUAL |n| '2)
                (|d02gbfDefaultSolve| |htPage| |a| |b| |mnp| |np| |lw|
                    |liw| |tol| |ifail|))
               ('T
                (setq |cList|
                         (PROG (G166940)
                           (setq G166940 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166940)
                               (SEQ (EXIT
                                     (SETQ G166940
                                      (APPEND G166940
                                       (|d02gbfSolve,fa| |i| |n|)))))))))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter the matrix {\\it D}: ")
                (setq |middle| (STRCONC |middle| '|\\newline |))
                (setq |dList|
                         (PROG (G166948)
                           (setq G166948 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166948)
                               (SEQ (EXIT
                                     (SETQ G166948
                                      (APPEND G166948
                                       (|d02gbfSolve,fc| |i| |n|)))))))))
                (setq |dList|
                         (CONS (CONS '|text| |middle|) |dList|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter the vector \\gamma: ")
                (setq |middle| (STRCONC |middle| '|\\newline |))
                (setq |gamList|
                         (PROG (G166956)
                           (setq G166956 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166956)
                               (SEQ (EXIT
                                     (SETQ G166956
                                      (APPEND G166956
                                       (|d02gbfSolve,fe| |i|)))))))))
                (setq |prefix| "\\newline ")
                (setq |gamList|
                         (CONS (CONS '|text| |middle|) |gamList|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter the matrix ")
                (setq |middle|
                         (STRCONC |middle|
                                  '|{\\it F(x)} from the equation {\\it y' =} |))
                (setq |middle|
                         (STRCONC |middle|
                                  '|{\\it F(x)y + g(x)}: \\newline |))
                (setq |fList|
                         (PROG (G166964)
                           (setq G166964 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166964)
                               (SEQ (EXIT
                                     (SETQ G166964
                                      (APPEND G166964
                                       (|d02gbfSolve,ff| |i| |n|)))))))))
                (setq |fList|
                         (CONS (CONS '|text| |middle|) |fList|))
                (setq |mid|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Enter the vector {\\it g(x)}: ")
                (setq |mid|
                         (STRCONC |mid| "\\newline "))
                (setq |gList|
                         (PROG (G166972)
                           (setq G166972 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |n|) G166972)
                               (SEQ (EXIT
                                     (SETQ G166972
                                      (APPEND G166972
                                       (|d02gbfSolve,fh| |i|)))))))))
                (setq |prefix| "\\newline ")
                (setq |gList|
                         (CONS (CONS '|text| |middle|) |gList|))
                (setq |xList|
                         (PROG (G166980)
                           (setq G166980 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| |mnp|) G166980)
                               (SEQ (EXIT
                                     (SETQ G166980
                                      (APPEND G166980
                                       (|d02gbfSolve,fi| |i|)))))))))
                (setq |end|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} The initial mesh {\\it X(mnp)}")
                (setq |end|
                         (STRCONC |end|
                                                                        ", (all entries = 0 if np < 4): \\newline "))
                (setq |xList| (CONS (CONS '|text| |end|) |xList|))
                (setq |equationPart|
                         (CONS '(|domainConditions|
                                    (|isDomain| EM |$EmptyMode|)
                                    (|isDomain| S (|String|))
                                    (|isDomain| F (|Float|))
                                    (|isDomain| I (|Integer|)))
                               (APPEND |cList|
                                       (APPEND |dList|
                                        (APPEND |gamList|
                                         (APPEND |fList|
                                          (APPEND |gList| |xList|)))))))
                (setq |page|
                         (|htInitPage|
                                                              "D02GBF - ODEs, boundary value problem, finite difference technique with deferred correction, general nonlinear problem"
                             NIL))
                (|htSay| "\\menuitemstyle{}\\tab{2} ")
                (|htSay|                              "Enter the matrix {\\it C} form the equation {\\it Cy(a) + Dy(b)} ")
                (|htSay| "= \\gamma \\newline ")
                (|htMakePage| |equationPart|)
                (|htMakeDoneButton| "Continue"
                    '|d02gbfGen|)
                (|htpSetProperty| |page| '|n| |n|)
                (|htpSetProperty| |page| '|a| |a|)
                (|htpSetProperty| |page| '|b| |b|)
                (|htpSetProperty| |page| '|mnp| |mnp|)
                (|htpSetProperty| |page| '|np| |np|)
                (|htpSetProperty| |page| '|lw| |lw|)
                (|htpSetProperty| |page| '|liw| |liw|)
                (|htpSetProperty| |page| '|tol| |tol|)
                (|htpSetProperty| |page| '|ifail| |ifail|)
                (|htpSetProperty| |page| '|inputArea|
                    (|htpInputAreaAlist| |htPage|))
