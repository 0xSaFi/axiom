(DEFUN |d02gbfSolve,fb| (|i| |j|)
  (PROG (|cnam|)
    (RETURN
      (SEQ (setq |cnam|
                    (INTERN (STRCONC "c" (princ-to-string |i|)
                                     (princ-to-string |j|))))
           (EXIT (CONS (CONS '|bcStrings|
                             (CONS (CONS 6
                                    (CONS 0
                                     (CONS |cnam| (CONS 'F NIL))))
                                   NIL))
                       NIL))))))

(DEFUN |d02gbfSolve,fa| (|i| |n|)
  (PROG (|prefix| |labelList|)
    (RETURN
      (SEQ (setq |labelList|
                    (PROG (G166859)
                      (setq G166859 NIL)
                      (RETURN
                        (DO ((|j| 1 (QSADD1 |j|)))
                            ((QSGREATERP |j| |n|) G166859)
                          (SEQ (EXIT (SETQ G166859
                                      (APPEND G166859
                                       (|d02gbfSolve,fb| |i| |j|)))))))))
           (setq |prefix| "\\newline ")
           (EXIT (setq |labelList|
                          (CONS (CONS '|text| |prefix|) |labelList|)))))))

