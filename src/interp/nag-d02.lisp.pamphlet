          (|text| . "\\newline\\tab{2} ") (|bcStrings| (6 11 |k| PI))
          (|text| . "\\blankline ")
          (|text| . "\\menuitemstyle{}\\tab{2}")
          (|text| . "Accuracy required {\\it tol}:")
          (|text| . "\\newline\\tab{2} ")
          (|bcStrings| (10 "0.0001" |tol| F)) (|text| . "\\blankline ")
          (|text| . "\\newline \\menuitemstyle{}\\tab{2} \\newline ")
