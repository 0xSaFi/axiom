                                      (CONS
                                       (CONS 42
                                        (CONS '|0.0|
                                         (CONS '|c1| (CONS 'EM NIL))))
                                       NIL))
                                     NIL)))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Value of {\\it q} for COEFFN:")
                (setq |middle| (STRCONC |middle| '|\\newline |))
                (setq |c1List|
                         (CONS (CONS '|text| |middle|)
                               (CONS (CONS '|bcStrings|
                                      (CONS
                                       (CONS 42
                                        (CONS '|0.0|
                                         (CONS '|c2| (CONS 'EM NIL))))
                                       NIL))
                                     NIL)))
                (setq |cList| (APPEND |cList| |c1List|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Value of {\\it dqdl}")
                (setq |middle|
                         (STRCONC |middle| '| for COEFFN: \\newline |))
                (setq |c2List|
                         (CONS (CONS '|text| |middle|)
                               (CONS (CONS '|bcStrings|
                                      (CONS
                                       (CONS 42
                                        (CONS '|0.0|
                                         (CONS '|c3| (CONS 'EM NIL))))
                                       NIL))
                                     NIL)))
                (setq |cList| (APPEND |cList| |c2List|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Values of YL(1) & YL(2) ")
                (setq |middle|
                         (STRCONC |middle| '|for BDYVAL: \\newline |))
                (setq |ylList|
                         (PROG (G167305)
                           (setq G167305 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| 2) G167305)
                               (SEQ (EXIT
                                     (SETQ G167305
                                      (APPEND G167305
                                       (|d02kefSolve,fb| |i|)))))))))
                (setq |ylList|
                         (CONS (CONS '|text| |middle|) |ylList|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Values of YR(1) & YR(2) ")
                (setq |middle|
                         (STRCONC |middle| '|for BDYVAL: \\newline |))
                (setq |yrList|
                         (PROG (G167313)
                           (setq G167313 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| 2) G167313)
                               (SEQ (EXIT
                                     (SETQ G167313
                                      (APPEND G167313
                                       (|d02kefSolve,fc| |i|)))))))))
                (setq |yrList|
                         (CONS (CONS '|text| |middle|) |yrList|))
                (setq |middle|
                                                      "\\blankline \\menuitemstyle{} \\tab{2} Maximum step size ")
                (setq |middle|
                         (STRCONC |middle|
                                  '|{\\it hmax(2,m)}: \\newline |))
                (setq |hList|
                         (PROG (G167321)
                           (setq G167321 NIL)
                           (RETURN
                             (DO ((|i| 1 (QSADD1 |i|)))
                                 ((QSGREATERP |i| 2) G167321)
                               (SEQ (EXIT
                                     (SETQ G167321
                                      (APPEND G167321
                                       (|d02kefSolve,fd| |i| |m|)))))))))
                (setq |hList|
                         (CONS (CONS '|text| |middle|) |hList|))
                (setq |equationPart|
                         (CONS '(|domainConditions|
                                    (|isDomain| EM |$EmptyMode|)
                                    (|isDomain| S (|String|))
                                    (|isDomain| F (|Float|))
                                    (|isDomain| I (|Integer|)))
                               (APPEND |xpList|
                                       (APPEND |cList|
                                        (APPEND |ylList|
                                         (APPEND |yrList| |hList|))))))
                (setq |page|
                         (|htInitPage|
                                                              "D02KEF - 2nd order Sturm-Liouville problem, regular/singular system, finite/infinite range, eigenvalue and eigenfunction, user-specified break-points"
