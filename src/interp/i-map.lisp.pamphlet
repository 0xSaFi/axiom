    (RETURN
      (SEQ (PROGN
             (setq |$freeVars| NIL)
             (setq |$localVars| NIL)
             (setq |$env| (CONS (CONS NIL NIL) NIL))
             (setq |$mapList| (CONS |op| |$mapList|))
