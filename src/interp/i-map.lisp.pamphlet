(DEFUN |analyzeUndeclaredMap| (|op| |argTypes| |mapDef| |$mapList|)
  (DECLARE (SPECIAL |$mapList|))
  (PROG (|$freeVars| |$localVars| |$env| |parms| |body| |n|)
    (DECLARE (SPECIAL |$freeVars| |$localVars| |$env| |$mapName|
