                        (setq G167716 NIL)
                        (RETURN
                          (DO ((G167723 |argTypes| (CDR G167723))
                               (|t1| NIL)
                               (G167724 (CDR |sig|) (CDR G167724))
                               (|t2| NIL)
                               (G167725 |parms| (CDR G167725))
                               (|arg| NIL))
                              ((OR (ATOM G167723)
                                   (PROGN
                                     (SETQ |t1| (CAR G167723))
                                     NIL)
                                   (ATOM G167724)
                                   (PROGN
                                     (SETQ |t2| (CAR G167724))
                                     NIL)
                                   (ATOM G167725)
                                   (PROGN
                                     (SETQ |arg| (CAR G167725))
                                     NIL))
