                                     NIL))
                               NIL)
                            (SEQ (EXIT (setq |sl|
                                        (|getEqualSublis,fn| |y| |sl|)))))
                          (EXIT |sl|))))
           (IF (AND (CONSP |x|) (EQ (QCAR |x|) '|is|)
                    (PROGN
                      (setq |ISTMP#1| (QCDR |x|))
                      (AND (CONSP |ISTMP#1|)
                           (PROGN
                             (setq |a| (QCAR |ISTMP#1|))
                             (setq |ISTMP#2| (QCDR |ISTMP#1|))
                             (AND (CONSP |ISTMP#2|)
                                  (EQ (QCDR |ISTMP#2|) NIL)
                                  (PROGN
                                    (setq |b| (QCAR |ISTMP#2|))
                                    'T))))))
               (EXIT (CONS (CONS |a| |b|) |sl|)))
           (IF (AND (CONSP |x|) (EQ (QCAR |x|) '=)
                    (PROGN
                      (setq |ISTMP#1| (QCDR |x|))
                      (AND (CONSP |ISTMP#1|)
                           (PROGN
                             (setq |a| (QCAR |ISTMP#1|))
                             (setq |ISTMP#2| (QCDR |ISTMP#1|))
                             (AND (CONSP |ISTMP#2|)
                                  (EQ (QCDR |ISTMP#2|) NIL)
                                  (PROGN
                                    (setq |b| (QCAR |ISTMP#2|))
