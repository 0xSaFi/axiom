                             (setq |l| (QCDR |ISTMP#1|))
                             'T)))
                    (|member| |op| '(|and| |or|)))
               (EXIT (SEQ (DO ((G167072 |l| (CDR G167072))
                               (|y| NIL))
                              ((OR (ATOM G167072)
                                   (PROGN
