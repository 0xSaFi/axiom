    (RETURN
      (SEQ (COND
             ((ATOM |form|) |form|)
             ('T (setq |op| (CAR |form|))
              (setq |args| (CDR |form|))
              (COND
                ((|member| |op| '(|exit| SEQ))
                 (CONS |op|
                       (PROG (G166943)
                         (setq G166943 NIL)
                         (RETURN
                           (DO ((G166948 |args| (CDR G166948))
                                (|a| NIL))
                               ((OR (ATOM G166948)
                                    (PROGN
                                      (SETQ |a| (CAR G166948))
                                      NIL))
                                (NREVERSE0 G166943))
                             (SEQ (EXIT (SETQ G166943
                                         (CONS
                                          (|simplifyMapConstructorRefs|
                                           |a|)
                                          G166943)))))))))
                ((|member| |op| '(REPEAT))
                 (CONS |op|
                       (CONS (CAR |args|)
                             (PROG (G166958)
                               (setq G166958 NIL)
                               (RETURN
                                 (DO ((G166963 (CDR |args|)
                                       (CDR G166963))
                                      (|a| NIL))
                                     ((OR (ATOM G166963)
                                       (PROGN
                                         (SETQ |a| (CAR G166963))
                                         NIL))
                                      (NREVERSE0 G166958))
                                   (SEQ
                                    (EXIT
                                     (SETQ G166958
                                      (CONS
                                       (|simplifyMapConstructorRefs|
                                        |a|)
                                       G166958))))))))))
                ((|member| |op| '(|:| |::| @))
                 (COND
                   ((AND (CONSP |args|)
                         (PROGN
                           (setq |obj| (QCAR |args|))
                           (setq |ISTMP#1| (QCDR |args|))
                           (AND (CONSP |ISTMP#1|)
                                (EQ (QCDR |ISTMP#1|) NIL)
                                (PROGN
                                  (setq |dom| (QCAR |ISTMP#1|))
                                  'T))))
                    (setq |dom'| (|prefix2String| |dom|))
                    (setq |dom''|
