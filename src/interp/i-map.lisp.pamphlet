                              ((NEQUAL (|objMode| |code|) |tar|)
                               (PROGN
                                 (setq |sigChanged| 'T)
                                 (setq |tar| (|objMode| |code|))
                                 (|restoreDependentMapInfo| |op|
                                     (CDR |$mapList|) |localMapInfo|))))))))
             (COND
               (|sigChanged|
                   (|throwKeyedMsg|
                    (format nil
                    "Axiom cannot determine the type for function %1 .~
                     Please declare it.")
                     (CONS |op| NIL)))
               ('T
                (|putMapCode| |op| (|objVal| |code|) |sig| |name|
                    |parms| 'T)
                (|genMapCode| |op| (|objVal| |code|) |sig| |name|
                    |parms| 'T)
                |tar|)))))))

