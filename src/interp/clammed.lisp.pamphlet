                                (G166285 |args| (CDR G166285))
                                (|x| NIL))
                               ((OR G166284 (ATOM G166285)
                                    (PROGN
                                      (SETQ |x| (CAR G166285))
                                      NIL))
                                G166278)
                             (SEQ (EXIT (SETQ G166278
                                         (AND G166278 (IDENTP |x|)))))))))
                 NIL)
                ((BOOT-EQUAL (|#| |args|) (|#| (REMDUP |args|))) 'T)
