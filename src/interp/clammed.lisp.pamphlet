                        (SPADLET |ISTMP#1| (QCDR |form|))
                        (AND (CONSP |ISTMP#1|)
                             (PROGN
                               (SPADLET |x| (QCAR |ISTMP#1|))
                               (SPADLET |ISTMP#2| (QCDR |ISTMP#1|))
                               (AND (CONSP |ISTMP#2|)
