(DEFUN |f07adfGen| (|htPage|)
  (PROG (|n| |m| |lda| |alist| |a| |y| |aList| |rowList| |astring|
             |prefix|)
    (RETURN
      (SEQ (PROGN
             (setq |n| (|htpProperty| |htPage| '|n|))
             (setq |m| (|htpProperty| |htPage| '|m|))
             (setq |lda| |m|)
             (setq |alist| (|htpInputAreaAlist| |htPage|))
             (setq |y| |alist|)
             (DO ((|i| 1 (QSADD1 |i|))) ((QSGREATERP |i| |n|) NIL)
               (SEQ (EXIT (PROGN
                            (DO ((|j| 1 (QSADD1 |j|)))
                                ((QSGREATERP |j| |m|) NIL)
                              (SEQ (EXIT
                                    (PROGN
                                      (setq |a|
                                       (STRCONC (ELT (CAR |y|) 1) '| |))
                                      (setq |rowList|
                                       (CONS |a| |rowList|))
                                      (setq |y| (CDR |y|))))))
                            (setq |aList| (CONS |rowList| |aList|))
                            (setq |rowList| NIL)))))
             (setq |astring|
                      (|bcwords2liststring|
                          (PROG (G166140)
                            (setq G166140 NIL)
                            (RETURN
                              (DO ((G166145 |aList| (CDR G166145))
                                   (|x| NIL))
                                  ((OR (ATOM G166145)
                                    (PROGN
                                      (SETQ |x| (CAR G166145))
                                      NIL))
                                   (NREVERSE0 G166140))
                                (SEQ (EXIT
                                      (SETQ G166140
                                       (CONS (|bcwords2liststring| |x|)
                                        G166140)))))))))
             (setq |prefix|
                      (STRCONC '|f07adf(| (princ-to-string |m|) '|, |
                               (princ-to-string |n|) '|, |))
             (setq |prefix|
                      (STRCONC |prefix| (princ-to-string |lda|) '|, |
                               |astring| '|)|))
             (|linkGen| |prefix|))))))

