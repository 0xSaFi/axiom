(DEFUN |f07aefGen| (|htPage|)
  (PROG (|n| |nrhs| |lda| |ldb| |length| |trans| |aplist| |b| |bList|
             |bstring| |ip| |ipList| |ipstring| |a| |y| |aList|
             |rowList| |astring| |prefix|)
    (RETURN
      (SEQ (PROGN
             (setq |n| (|htpProperty| |htPage| '|n|))
             (setq |nrhs| (|htpProperty| |htPage| '|nrhs|))
             (setq |lda| |n|)
             (setq |ldb| |n|)
             (setq |length| (|htpProperty| |htPage| '|length|))
             (setq |trans| (|htpProperty| |htPage| '|trans|))
             (setq |aplist| (|htpInputAreaAlist| |htPage|))
             (setq |y| |aplist|)
             (DO ((|i| 1 (QSADD1 |i|))) ((QSGREATERP |i| |n|) NIL)
               (SEQ (EXIT (PROGN
                            (DO ((|j| 1 (QSADD1 |j|)))
                                ((QSGREATERP |j| |nrhs|) NIL)
                              (SEQ (EXIT
                                    (PROGN
                                      (setq |b|
                                       (STRCONC (ELT (CAR |y|) 1) '| |))
                                      (setq |rowList|
                                       (CONS |b| |rowList|))
                                      (setq |y| (CDR |y|))))))
                            (setq |bList| (CONS |rowList| |bList|))
                            (setq |rowList| NIL)))))
             (setq |bstring|
                      (|bcwords2liststring|
                          (PROG (G166320)
                            (setq G166320 NIL)
                            (RETURN
                              (DO ((G166325 |bList| (CDR G166325))
                                   (|x| NIL))
                                  ((OR (ATOM G166325)
                                    (PROGN
                                      (SETQ |x| (CAR G166325))
                                      NIL))
                                   (NREVERSE0 G166320))
                                (SEQ (EXIT
                                      (SETQ G166320
                                       (CONS (|bcwords2liststring| |x|)
                                        G166320)))))))))
             (DO ((|i| 1 (QSADD1 |i|))) ((QSGREATERP |i| |length|) NIL)
               (SEQ (EXIT (PROGN
                            (setq |ip|
                                     (STRCONC (ELT (CAR |y|) 1) '| |))
                            (setq |ipList| (CONS |ip| |ipList|))
                            (setq |y| (CDR |y|))))))
             (setq |ipstring| (|bcwords2liststring| |ipList|))
             (DO ((|i| 1 (QSADD1 |i|))) ((QSGREATERP |i| |lda|) NIL)
               (SEQ (EXIT (PROGN
                            (DO ((|j| 1 (QSADD1 |j|)))
                                ((QSGREATERP |j| |n|) NIL)
                              (SEQ (EXIT
                                    (PROGN
                                      (setq |a|
                                       (STRCONC (ELT (CAR |y|) 1) '| |))
                                      (setq |rowList|
                                       (CONS |a| |rowList|))
                                      (setq |y| (CDR |y|))))))
                            (setq |aList| (CONS |rowList| |aList|))
                            (setq |rowList| NIL)))))
             (setq |astring|
                      (|bcwords2liststring|
                          (PROG (G166365)
                            (setq G166365 NIL)
                            (RETURN
                              (DO ((G166370 |aList| (CDR G166370))
                                   (|x| NIL))
                                  ((OR (ATOM G166370)
                                    (PROGN
                                      (SETQ |x| (CAR G166370))
                                      NIL))
                                   (NREVERSE0 G166365))
                                (SEQ (EXIT
                                      (SETQ G166365
                                       (CONS (|bcwords2liststring| |x|)
                                        G166365)))))))))
             (setq |prefix|
                      (STRCONC '|f07aef("| |trans| '|", |
                               (princ-to-string |n|) '|, |))
             (setq |prefix|
                      (STRCONC |prefix| (princ-to-string |nrhs|) '|, |
                               |astring| '|::Matrix DoubleFloat, |))
             (setq |prefix|
                      (STRCONC |prefix| (princ-to-string |lda|) '|, [|
                               |ipstring| '|]::Matrix INT, |))
             (setq |prefix|
                      (STRCONC |prefix| (princ-to-string |ldb|) '|, |
                               |bstring| '|::Matrix DoubleFloat)|))
             (|linkGen| |prefix|))))))

