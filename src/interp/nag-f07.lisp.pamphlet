          (|text| . "\\blankline ")
          (|text| . "\\menuitemstyle{}\\tab{2}")
          (|text| . "Number of columns {\\it n}:")
          (|text| . "\\newline\\tab{2} ") (|bcStrings| (5 4 |n| PI))))
    (|htMakeDoneButton| "Continue" '|f07adfSolve|)
    (|htShowPage|)))

