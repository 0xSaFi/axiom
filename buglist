fixed 7057: summation is ambiguous

summation(i^2,i=a..b)^(d-c)

         b      d - c
        --+    2
   (1)  >     i
        --+
        i= a

summation(i^2^(d-c),i=a..b)

         b      d - c
        --+    2
   (2)  >     i
        --+
        i= a

sum(operator(f) (i)+1,i=1..n)

         n
        --+
   (3)  >     f(i) + 1
        --+
        i= 1

sum(operator(f) (i),i=1..n)+1

         n
        --+
   (4)  >     f(i) + 1
        --+
        i= 1

sum(operator(f) (i)+1,i=1..n)^3

         n            3
        --+
   (5)  >     f(i) + 1
        --+
