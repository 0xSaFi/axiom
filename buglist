   "kaf1405.sdata"

(1) -> 
patrons."Smith" := 10500 
patrons."Jones" := 22000
patrons."Jones" 
patrons."Stingy"
reduce(+, entries patrons) 
)system rm -r kaf*.sdata
)spool
)lisp (bye)

